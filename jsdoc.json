{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./"],
    "exclude": [
      "docs",
      "test",
      "input",
      "output",
      "node_modules"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "node_modules/jsdoc-strip-async-await"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
