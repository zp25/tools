{
  "name": "tools",
  "version": "0.1.3",
  "description": "cli tools",
  "keywords": [
    "imagemin",
    "base64",
    "today",
    "zxcvbn"
  ],
  "main": "index.js",
  "bin": {
    "tools": "./bin/cli.js"
  },
  "scripts": {
    "upgrade-interactive": "npm-check -u",
    "clean": "rimraf output/*",
    "test": "nyc mocha --harmony --check-leaks",
    "prejsdoc": "rimraf docs/*",
    "jsdoc": "jsdoc -r -c jsdoc.json -d docs",
    "open": "open docs/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zp25/tools.git"
  },
  "author": "zp25",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.1.0",
    "imagemin-webp": "^5.1.0",
    "ora": "^4.0.3",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "husky": "^3.1.0",
    "jsdoc": "^3.6.3",
    "jsdoc-strip-async-await": "^0.1.0",
    "mocha": "^6.2.2",
    "npm-check": "^5.9.0",
    "nyc": "^15.0.0",
    "rewire": "^4.0.1",
    "rimraf": "^3.0.0",
    "sinon": "^8.0.2"
  },
  "engines": {
    "node": ">=10.18.0"
  }
