    "imagemin-svgo": "^5.2.2",
    "imagemin-webp": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc": "^3.5.4",
    "jsdoc-strip-async-await": "^0.1.0",
    "mocha": "^3.5.0",
